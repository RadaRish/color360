{
  "env": {
    "browser": true,
    "es2021": true,
    "jest": true
  },
  "parserOptions": {
    "ecmaVersion": 2021,
    "sourceType": "module"
  },
  "globals": {
    "AFRAME": "readonly",
    "THREE": "readonly"
  },
  "rules": {
    "no-undef": "off",
    "no-unused-vars": "off",
    "no-console": "off",
    "prefer-const": "off"
  }
}
